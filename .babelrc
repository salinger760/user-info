{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "modules": "auto"
        },
        "transform-runtime": {
          "useESModules": false
        }
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true
      }
    ]
  ]
}
